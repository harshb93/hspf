#/bin/csh

set usrdir = ../../../lib
set usrlib = ($usrdir/ttyux.o $usrdir/get_lib.a $usrdir/util_lib.a)

f77 -c main.f
f77 -c delatdepann.f
f77 -c delatdepave.f
f77 -c delatdepmon.f
f77 -c deldatann.f
f77 -c deldatave.f
f77 -c deldatmon.f
f77 -c delinfo.f
f77 -c dellsegann.f
f77 -c dellsegave.f
f77 -c dellsegmon.f
f77 -c getdfs.f
f77 -c getloads.f
f77 -c getRvars.f
f77 -c readcontrol_dat.f

f77 -o ../../../../bin/DEL.exe main.o delatdepann.o delatdepave.o delatdepmon.o deldatann.o deldatave.o deldatmon.o delinfo.o dellsegann.o dellsegave.o dellsegmon.o getdfs.o getloads.o getRvars.o readcontrol_dat.o $usrlib

rm *.o

